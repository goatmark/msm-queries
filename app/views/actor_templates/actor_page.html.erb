<% params %>

<h1>Actor #<%=@actor_id%> details</h1>

<hr>

<dl>
  <dt>
    Name
  </dt>
  <dd>
    <%=@specific_actor.name%>
  </dd>

  <dt>
    Dob
  </dt>
  <dd>
    <%=@specific_actor.dob%>
  </dd>

  <dt>
    Bio
  </dt>
  <dd>
    <%=@specific_actor.bio%>
  </dd>

  <dt>
    Image
  </dt>
  <dd>
    <img src="<%=@specific_actor.image%>" style="width:250px;">
  </dd>

  <dt>
    Created at
  </dt>
  <dd>
    <%=@specific_actor.created_at%>
  </dd>

  <dt>
    Updated at
  </dt>
  <dd>
    <%=@specific_actor.updated_at%>
  </dd>
</dl>

<hr>
